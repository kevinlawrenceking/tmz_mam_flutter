{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug (Windows)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "${workspaceFolder}/lib/main.dart"
    },
    {
      "name": "Debug (Chrome)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "${workspaceFolder}/lib/main.dart",
      "args": ["-d", "chrome", "--web-renderer", "canvaskit"]
    },
    {
      "name": "Debug (Edge)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "${workspaceFolder}/lib/main.dart",
      "args": ["-d", "edge", "--web-renderer", "canvaskit"]
    }
  ]
}
